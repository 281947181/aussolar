<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.zc.aussolar.bean.AdditionalComment" table="tbl_additional_comment">
		<id name="vid">
			<generator class="native"></generator>
		</id>
		<property name="project_id"></property>
		<property name="comment_date"></property>
		<property name="comment_person"></property>
		<property name="comment"></property>
	</class>
</hibernate-mapping>
	