<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.zc.aussolar.bean.LeadsModifyRecord" table="tbl_leads_modify_record">
		<id name="vid">
			<generator class="native"></generator>
		</id>
		<property name="project_id"></property>
		<property name="status"></property>
		
		<property name="create_date"></property>
		<property name="create_person"></property>
		<property name="leads_type"></property>
		<property name="leads_comment"></property>
		<property name="leads_from"></property>
		<property name="leads_knocker"></property>
		<property name="customer_name"></property>
		<property name="customer_phone"></property>
		<property name="customer_address"></property>
		<property name="customer_address_lat"></property>
		<property name="customer_address_lng"></property>
		<property name="customer_subburb"></property>
		<property name="customer_postcode"></property>
		<property name="customer_state"></property>
		<property name="customer_email"></property>
	</class>
</hibernate-mapping>
	